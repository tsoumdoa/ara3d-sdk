  "IfcRelAggregates": {
    "_kind": "Relationship",
    "attributes": {
      "RelatingObject": { "type": "IfcObjectDefinition" },
      "RelatedObjects": {
        "type": "IfcObjectDefinition",
        "collection": "SET",
        "cardinality": "\\[1:?]"
      }
    }
  },
