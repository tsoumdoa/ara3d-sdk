      "RelatedBuildingElement": { "type": "IfcElement" }
    }
  },

  "IfcProductDefinitionShape": {
    "_kind": "Representation container",
    "attributes": {
      "Representations": {
        "type": "IfcShapeRepresentation",
        "collection": "LIST",
        "cardinality": "\\[1:?]"
      }
    }
  },
  "IfcShapeRepresentation": {
    "_kind": "Representation",
    "attributes": {
      "ContextOfItems": { "type": "IfcRepresentationContext" },
      "RepresentationIdentifier": {
        "type": "IfcLabel",
        "examples": "\\",
        [ "Body", "FootPrint", "Box", "Axis", "Reference", "Annotation" ]
      },
      "RepresentationType": {
        "type": "IfcLabel",
        "examples": "\\",
        [ "SweptSolid", "Clipping", "Brep", "MappedRepresentation", "Curve2D", "SurfaceModel", "GeometricSet" ]
      },
      "Items": {
        "type": "IfcRepresentationItem",
        "collection": "SET",
        "cardinality": "\\[1:?]"
      }
    }
  },
  "IfcGeometricRepresentationSubContext": {
    "_kind": "Representation context (sub)",
    "attributes": {
      "ParentContext": { "type": "IfcGeometricRepresentationContext" },
      "ContextIdentifier": {
        "type": "IfcLabel",
        "optional": true
      },
      "ContextType": {
        "type": "IfcLabel",
        "optional": true
      },
      "TargetView": {
        "type": "IfcGeometricProjectionEnum",
        "optional": true,
        "examples": "\\",
        [ "PLAN\\_VIEW", "MODEL\\_VIEW" ]
      }
    }
  },
  "IfcLocalPlacement": {
    "_kind": "Placement",
    "attributes": {
      "PlacementRelTo": {
        "type": "IfcObjectPlacement",
        "optional": true
      },
      "RelativePlacement": { "type": "IfcAxis2Placement2D/3D" }
    }
  },
  "IfcBoundingBox": {
    "_kind": "Simple shape (AABB in local coords)",
    "attributes": {
      "Corner": { "type": "IfcCartesianPoint" },
      "XDim": { "type": "IfcPositiveLengthMeasure" },
      "YDim": { "type": "IfcPositiveLengthMeasure" },
      "ZDim": { "type": "IfcPositiveLengthMeasure" }
    }
  },
  "IfcMappedItem": {
    "_kind": "Reuse/instancing",
    "attributes": {
      "MappingSource": { "type": "IfcRepresentationMap" },
      "MappingTarget": { "type": "IfcCartesianTransformationOperator2D/3D" }
    }
  }
}
