  },
  "IfcRelSpaceBoundary": {
    "_kind": "Relationship (base)",
    "attributes": {
      "RelatingSpace": { "type": "IfcSpaceBoundarySelect" },
      "RelatedBuildingElement": {
        "type": "IfcElement",
        "optional": true
      },
      "ConnectionGeometry": {
        "type": "IfcConnectionGeometry",
        "optional": true
      },
      "PhysicalOrVirtualBoundary": { "type": "IfcPhysicalOrVirtualEnum" },
      "InternalOrExternalBoundary": { "type": "IfcInternalOrExternalEnum" }
    }
  },
  "IfcRelSpaceBoundary1stLevel": {
    "_kind": "Relationship (adds segmentation)",
    "attributes": {
      "RelatingSpace": { "type": "IfcSpaceBoundarySelect" },
      "RelatedBuildingElement": {
        "type": "IfcElement",
        "optional": true
      },
      "ConnectionGeometry": {
        "type": "IfcConnectionGeometry",
        "optional": true
      },
      "PhysicalOrVirtualBoundary": { "type": "IfcPhysicalOrVirtualEnum" },
      "InternalOrExternalBoundary": { "type": "IfcInternalOrExternalEnum" },
      "ParentBoundary": {
        "type": "IfcRelSpaceBoundary1stLevel",
        "optional": true
      },
      "InnerBoundaries": {
        "type": "IfcRelSpaceBoundary1stLevel",
        "collection": "SET",
        "cardinality": "\\[0:?]"
      }
    }
  },
  "IfcRelSpaceBoundary2ndLevel": {
    "_kind": "Relationship (adds pairing across elements)",
    "attributes": {
      "RelatingSpace": { "type": "IfcSpaceBoundarySelect" },
      "RelatedBuildingElement": {
        "type": "IfcElement",
        "optional": true
      },
      "ConnectionGeometry": {
        "type": "IfcConnectionGeometry",
